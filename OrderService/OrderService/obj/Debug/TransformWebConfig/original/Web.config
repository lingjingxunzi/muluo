<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <pages validateRequest="false" />
    <httpRuntime requestValidationMode="2.0" />
  </system.web>
  <system.serviceModel>
    <bindings />
    <client />
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- 为避免泄漏元数据信息，请在部署前将以下值设置为 false 并删除上面的元数据终结点 -->
          <serviceMetadata httpGetEnabled="true"/>
          <!-- 要接收故障异常详细信息以进行调试，请将以下值设置为 true。在部署前设置为 false 以避免泄漏异常信息 -->
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
  </system.webServer>


  <appSettings>
    <add key="JTDisUrl" value="http://interface.5liuba.com"  />
    <add key="JTHomeUrl" value="http://interface.5liuba.com"  />
    <add key="XYDisUrl" value="http://interface.5liuba.com"  />
    <add key="XYHomeUrl" value="http://interface.5liuba.com"  />
    <add key="SXDDisUrl" value="http://113.207.124.143/Order/SxdCallBack.aspx"/>
    <add key="SXDHomeUrl" value="http://www.5liuba.net/"/>
  </appSettings>
</configuration>
