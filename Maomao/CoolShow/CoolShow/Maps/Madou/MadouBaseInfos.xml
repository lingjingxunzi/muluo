  <!--动态sql语句-->
  <statements>
    <!-- Insert statement.-->
    <insert id='InsertBusinesserBaseInfo' parameterClass='MadouBaseInfos'>
      INSERT INTO MadouBaseInfos
      (
      Nick,
      AgeRange,
      ImageStyle,
      JoinDate,
      QQNumber,
      Occupation
      )
      VALUES
      (
      #Nick#,
      #AgeRange#,
      #ImageStyle#,
      #JoinDate#,
      #QQNumber#,
      #Occupation#

      )
    </insert>


    <update id='UpdateBusinesserBaseInfo' parameterClass='MadouBaseInfos'>
      UPDATE MadouBaseInfos
      SET
      <isNotEmpty prepend="," property="AgeRange">AgeRange=#AgeRange#</isNotEmpty>
      <isNotEmpty prepend="," property="Occupation">Occupation=#Occupation#</isNotEmpty>
      <isNotEmpty prepend="," property="ImageStyle">ImageStyle=#ImageStyle#</isNotEmpty>
      <isNotEmpty prepend="," property="JoinDate">JoinDate=#JoinDate#</isNotEmpty>


      WHERE
      [MadouBaseInfoKey] = #MadouBaseInfoKey#
    </update>

    <delete id='DeleteBusinesserBaseInfo' parameterClass='MadouBaseInfos'>
      DELETE FROM  MadouBaseInfos
      WHERE
      [MadouBaseInfoKey] = #MadouBaseInfoKey#
    </delete>

    <!--动态查询语句-->
    <select id="SelectBusinesserBaseInfoList" parameterClass="MadouBaseInfos"  resultMap="FB_MadouBaseInfosResult">
      <![CDATA[SELECT 
          MadouBaseInfoKey,
          Nick,
         AgeRange,
         Occupation,
         ImageStyle,
         JoinDate
         ]]>
      <dynamic prepend="">
        <isEqual prepend="" property="IsStartPager" compareValue="true">
          FROM (SELECT ROW_NUMBER() OVER(ORDER BY MadouBaseInfoKey DESC) AS RowNumber, *
          FROM  MadouBaseInfos
          WHERE 1=1
          <isNotEmpty prepend="AND" property="AgeRange"  > AgeRange=#AgeRange#</isNotEmpty>
          <isNotEmpty prepend="AND" property="JoinDate"> JoinDate=#JoinDate#</isNotEmpty>
          <isNotEmpty prepend="AND"  property="ImageStyle">ImageStyle=#ImageStyle#</isNotEmpty>

        </isEqual>
        )T
        <isEqual prepend="" property="IsStartPager" compareValue="false">
          FROM  MadouBaseInfos
        </isEqual>
      </dynamic>
      where 1=1
      <dynamic >
        <isNotEmpty prepend="AND" property="AgeRange"  >AgeRange=#AgeRange#</isNotEmpty>
        <isNotEmpty prepend="AND" property="JoinDate">JoinDate=#JoinDate#</isNotEmpty>
        <isNotEmpty prepend="AND"  property="ImageStyle">ImageStyle=#ImageStyle#</isNotEmpty>
        <isGreaterThan prepend="AND" property="StartRecordIndex" compareValue="0">
          <![CDATA[RowNumber>=#StartRecordIndex#]]>
        </isGreaterThan>
        <isGreaterThan prepend="AND" property="EndRecordIndex" compareValue="0">
          <![CDATA[RowNumber<=#EndRecordIndex#]]>
        </isGreaterThan>
      </dynamic>

    </select>

    <select id="SelectBusinesserBaseInfoCount" parameterClass="MadouBaseInfos" resultClass="int">
      Select count(*)
      FROM  MadouBaseInfos
      WHERE 1=1
      <dynamic >
        <isNotEmpty prepend="AND" property="AgeRange" compareValue="0">AgeRange=#AgeRange#</isNotEmpty>
        <isNotEmpty prepend="AND" property="JoinDate">JoinDate=#JoinDate#</isNotEmpty>
      </dynamic>
    </select>

    <select id="SelectBusinesserBaseInfoByKey" parameterClass="MadouBaseInfos" resultMap="FB_MadouBaseInfosResult">
      Select
      MadouBaseInfoKey,
      Nick,
      AgeRange,
      Occupation,
      ImageStyle,
      JoinDate,
      QQNumber
      FROM  MadouBaseInfos
      WHERE MadouBaseInfoKey=#MadouBaseInfoKey#
    </select>


    <select id="SelectBusinesserBaseInfoByNick" parameterClass="MadouBaseInfos" resultMap="FB_MadouBaseInfosResult">
      Select
      MadouBaseInfoKey,
      Nick,
      AgeRange,
      Occupation,
      ImageStyle,
      JoinDate,
      QQNumber
      FROM  MadouBaseInfos
      WHERE Nick=#Nick#
    </select>






  </statements>
